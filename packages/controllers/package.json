{
  "name": "@abb/controllers",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "download-schema": "apollo schema:download --endpoint http://localhost:4000",
    "generate": "apollo codegen:generate --queries src/**/*.tsx --schema schema.json --target typescript --addTypename",
    "gen-types": "npm run download-schema && npm run generate"
  },
  "dependencies": {
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "react": "^16.4.1",
    "react-apollo": "^2.1.9",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@types/node": "^10.5.2",
    "@types/react": "16.4.2",
    "@types/react-dom": "^16.0.6",
    "apollo": "^1.4.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.9.2"
  }
}
